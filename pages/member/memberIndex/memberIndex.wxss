page {
  background: rgba(250, 250, 250, 1);
}

swiper {
  min-height: 1060rpx;
}

.nav {
  margin-top: 70rpx;
  padding: 0 40rpx;
  font-size: 32rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  display: flex;
}

.nav .nav-item {
  padding-bottom: 20rpx;
}

.nav .nav-item:last-of-type {
  margin-left: 50rpx;
}

.btm_line {
  border-bottom: 6rpx solid #FF2442;
}

.hade {
  width: 670rpx;
  height: 156rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 6rpx 28rpx 0rpx rgba(0, 0, 0, 0.05);
  border-radius: 16rpx;
  margin: 66rpx 40rpx 40rpx 40rpx;
  display: flex;
  align-items: center;
}

.hade .head-item {
  width: 33.333%;
}

.hade .head-item .top {
  font-size: 36rpx;
  font-family: DIN;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  text-align: center;
}

.hade .head-item .bottom {
  font-size: 24rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  margin-top: 15rpx;
  text-align: center;
}

.memberList {
  width: 670rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 6rpx 28rpx 0rpx rgba(0, 0, 0, 0.05);
  border-radius: 16rpx;
  margin: 0 40rpx 196rpx 40rpx;
}

.memberList .member-item {
  position: relative;
  padding-left: 132rpx;
  display: flex;
  justify-content: space-between;
  padding-top: 40rpx;
  padding-bottom: 30rpx;
  padding-right: 40rpx;
}

.memberList .member-item .icon {
  width: 80rpx;
  height: 80rpx;
  background: rgba(67, 137, 68, 1);
  border-radius: 40rpx;
  position: absolute;
  left: 40rpx;
  top: 40rpx;
}

.memberList .member-item .left .user-name {
  width: 336rpx;
}

.memberList .member-item .left .user-name,
.memberList .member-item .right .price {
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.memberList .member-item .left .address,
.memberList .member-item .right .sum {
  font-size: 24rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  margin-top: 15rpx;
}

.memberList .member-item .right .sum {
  text-align: right;
}

.head1 {
  width: 670rpx;
  height: 100rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 6rpx 28rpx 0rpx rgba(0, 0, 0, 0.05);
  border-radius: 16rpx;
  margin: 66rpx 40rpx 40rpx 40rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 40rpx;
}

.head1 .left {
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.head1 .right {
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  display: flex;
  align-items: center;
  height: 28rpx;
}

.head1 .right .tz {
  width: 16rpx;
  height: 28rpx;
  margin-left: 20rpx;
}

.zk-list {
  width: 670rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 6rpx 28rpx 0rpx rgba(0, 0, 0, 0.05);
  border-radius: 16rpx;
  margin: 0 40rpx 196rpx 40rpx;
  padding: 40rpx;
}

.zk-list .title {
  font-size: 32rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.zk-list .item {
  margin-top: 40rpx;
  padding: 30rpx;
  position: relative;
  width: 590rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 8rpx;
}

.zk-list .item .v1 {
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.zk-list .item .v2 {
  font-size: 24rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  margin-top: 20rpx;
}

.zk-list .item .v3 {
  font-size: 24rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  margin-top: 20rpx;
}

.zk-list .item .tz {
  width: 16rpx;
  height: 28rpx;
  position: absolute;
  top: 74rpx;
  right: 30rpx;
}