page {
  background: rgba(250, 250, 250, 1);
}

.head {
  width: 670rpx;
  height: 80rpx;
  background: rgba(246, 232, 194, 1);
  border-radius: 16rpx;
  margin: 40rpx;
  background-color: #F6E8C2;
  display: flex;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.head .iocn {
  margin-left: 30rpx;
  margin-right: 20rpx;
  width: 36rpx;
  height: 36rpx;
  background-color: #D7B566;
  text-align: center;
  line-height: 36rpx;
  border-radius: 50%;
  font-size: 24rpx;
  color: #fff;
}

.crad {
  width: 670rpx;
  height: 672rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 6rpx 28rpx 0rpx rgba(0, 0, 0, 0.05);
  border-radius: 16rpx;
  margin: 0 40rpx 40rpx 40rpx;
  padding: 60rpx 40rpx;
  background-color: #fff;
}

.crad .v1 {
  margin: 0 auto;
  width: 260rpx;
  height: 200rpx;
  border-radius: 8rpx;
  border: 2rpx dashed rgba(151, 151, 151, 1);
}

.crad .v1 .img {
  width: 50rpx;
  height: 50rpx;
  margin: 50rpx 106rpx 24rpx 106rpx;
}

.crad .v1 view {
  font-size: 24rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-align: center;
  letter-spacing: 2rpx;
}

.crad .v2 {
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
}

.crad .left {
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  text-align: justify;
  width: 120rpx;
}

.crad .right {
  text-align: right;
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  padding-right: 10rpx;
  width: 470rpx;
}

.addpre {
  margin-left: 40rpx;
  display: flex;
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(255, 36, 66, 1);
  align-items: center;
}

.addpre image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
}

.submit {
  width: 670rpx;
  height: 88rpx;
  background: linear-gradient(90deg, rgba(255, 128, 128, 1) 0%, rgba(255, 36, 66, 1) 100%);
  border-radius: 44rpx;
  margin: 60rpx 40rpx;
  text-align: center;
  line-height: 88rpx;
  font-size: 32rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}